{
	"name" : "sas-irad/penngroups-bundle",
	"type" : "library",
	"description" : "Perform penngroups lookups via LDAP/WebService",
	"keywords" : [
		"symfony",
		"irad",
		"penngroups",
		"ldap",
		"pennkey",
		"penn_id"
	],
	"homepage" : "http://www.sas.upenn.edu",
	"license" : "MIT",
	"authors" : [{
			"name" : "Roberto Mansfield",
			"email" : "robertom@sas.upenn.edu"
		}
	],
	"require" : {
		"php" : ">=5.3.2",
		"sas-irad/file-storage-bundle" : ">=1.0.0",
        "sas-irad/person-info-bundle" : ">=1.0.0"
	},
	"require-dev" : {
		"symfony/framework-bundle" : ">=2.4,<2.5-dev",
		"symfony/class-loader" : ">=2.4,<2.5-dev",
		"symfony/console" : ">=2.4,<2.5-dev",
		"symfony/debug" : ">=2.4,<2.5-dev",
		"symfony/finder" : ">=2.4,<2.5-dev",
		"symfony/yaml" : ">=2.4,<2.5-dev"
	},
	"minimum-stability" : "stable",
	"autoload" : {
		"psr-4" : {
			"SAS\\IRAD\\PennGroupsBundle\\" : ""
		}
	},
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/sas-irad/file-storage-bundle"
		}, {
			"type" : "vcs",
			"url" : "https://github.com/sas-irad/person-info-bundle"
		}
	]
